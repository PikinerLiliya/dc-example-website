<div class="amp-ca-product-image-holder">
    <picture class="amp-ca-product-image">
        <source srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}.webp?w=1600{{#compare display '==' 'Point of Interest'}}&$poi$&aspect=2.6:1{{/compare}}&upscale=false&fmt.jpeg.interlaced=true 1x, //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}.webp?w=3200{{#compare display '==' 'Point of Interest'}}&$poi$&aspect=2.6:1{{/compare}}&upscale=false&fmt.jpeg.interlaced=true 2x" media="(min-width: 1280px)" type="image/webp">
        <source srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}?w=1600{{#compare display '==' 'Point of Interest'}}&$poi$&aspect=2.6:1{{/compare}}&upscale=false&fmt.jpeg.interlaced=true 1x, //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}?w=3200{{#compare display '==' 'Point of Interest'}}&$poi$&aspect=2.6:1{{/compare}}&upscale=false&fmt.jpeg.interlaced=true 2x" media="(min-width: 1280px)">

        <source srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}.webp?w=1280{{#compare display '==' 'Point of Interest'}}&$poi$&aspect=2:1{{/compare}}&upscale=false&fmt.jpeg.interlaced=true 1x, //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}.webp?w=2560{{#compare display '==' 'Point of Interest'}}&$poi$&aspect=2:1{{/compare}}&upscale=false&fmt.jpeg.interlaced=true 2x" media="(min-width: 1024px)" type="image/webp">
        <source srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}?w=1280{{#compare display '==' 'Point of Interest'}}&$poi$&aspect=2:1{{/compare}}&upscale=false&fmt.jpeg.interlaced=true 1x, //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}?w=2560{{#compare display '==' 'Point of Interest'}}&$poi$&aspect=2:1{{/compare}}&upscale=false&fmt.jpeg.interlaced=true 2x" media="(min-width: 1024px)">

        <source srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}.webp?w=1024{{#compare display '==' 'Point of Interest'}}&$poi$&aspect=1.5:1{{/compare}}&upscale=false&fmt.jpeg.interlaced=true 1x, //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}.webp?w=2048{{#compare display '==' 'Point of Interest'}}&$poi$&aspect=1.5:1{{/compare}}&upscale=false&fmt.jpeg.interlaced=true 2x" media="(min-width: 768px)" type="image/webp">
        <source srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}?w=1024{{#compare display '==' 'Point of Interest'}}&$poi$&aspect=1.5:1{{/compare}}&upscale=false&fmt.jpeg.interlaced=true 1x, //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}?w=2048{{#compare display '==' 'Point of Interest'}}&$poi$&aspect=1.5:1{{/compare}}&upscale=false&fmt.jpeg.interlaced=true 2x" media="(min-width: 768px)">

        <source srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}.webp?w=768{{#compare display '==' 'Point of Interest'}}&$poi$&aspect=1:1{{/compare}}&upscale=false&fmt.jpeg.interlaced=true 1x, //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}.webp?w=1536{{#compare display '==' 'Point of Interest'}}&$poi$&aspect=1:1{{/compare}}&upscale=false&fmt.jpeg.interlaced=true 2x" media="(max-width: 768px)" type="image/webp">
        <source srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}?w=768{{#compare display '==' 'Point of Interest'}}&$poi$&aspect=1:1{{/compare}}&upscale=false&fmt.jpeg.interlaced=true 1x, //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}?w=1536{{#compare display '==' 'Point of Interest'}}&$poi$&aspect=1:1{{/compare}}&upscale=false&fmt.jpeg.interlaced=true 2x" media="(max-width: 768px)">
        <img src="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}?w=1600{{#compare display '==' 'Point of Interest'}}&$poi$&aspect=2.6:1{{/compare}}&upscale=false&fmt.jpeg.interlaced=true" srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}.webp?w=1600{{#compare display '==' 'Point of Interest'}}&$poi$&aspect=2.6:1{{/compare}}&upscale=false&fmt.jpeg.interlaced=true 1x, //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}.webp?w=3200{{#compare display '==' 'Point of Interest'}}&$poi$&aspect=2.6:1{{/compare}}&upscale=false&fmt.jpeg.interlaced=true 2x,
                 //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}?w=1600{{#compare display '==' 'Point of Interest'}}&$poi$&aspect=2.6:1{{/compare}}&upscale=false&fmt.jpeg.interlaced=true 1x, //{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}{{#if name}}/{{escape name}}{{/if}}?w=3200{{#compare display '==' 'Point of Interest'}}&$poi$&aspect=2.6:1{{/compare}}&upscale=false&fmt.jpeg.interlaced=true 2x" class="amp-ca-image-pic" {{#if name}} alt="{{name}}" {{/if}} {{#if name}} title="{{name}}" {{/if}} />
    </picture>
    <div class="amp-ca-product-attribute-holder">
        <div class="amp-ca-product-volume-holder"><div class="amp-ca-product-volume-text-holder">{{volume}} {{unit}}</div></div>
        <div class="amp-ca-product-category-holder"><div class="amp-ca-product-category-text-holder">{{category}}</div></div>
        
    </div>
    {{#compare display '==' 'Hotspots'}}
    <div class="amp-ca-image-actions" data-amp-image-actions="//{{image.defaultHost}}/i/{{image.endpoint}}/{{image.name}}.json?metadata=true&v="></div>
    {{/compare}}
</div>